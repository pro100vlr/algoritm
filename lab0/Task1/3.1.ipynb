    "if not(-(10**9)<=a<=10**9) or not(-(10)**9<=b<=10**9):\n",
    "    print(\"Неверные значения переменных\")\n",
    "    exit()\n",
    "f=open('output.txt', 'w')\n",
    "print(int(a)+int(b), file=f)\n",
    "f.close()"
   ]
